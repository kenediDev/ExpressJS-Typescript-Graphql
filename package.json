  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ts": "ts-node server/server.ts",
    "nodemon": "rm -rf dist && rm server/logger/access.log && nodemon server/server.ts",
    "jest": "cross-env test=true jest --watchAll server/**/*.test.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm",
    "entities": "npm run typeorm -- entity:create --config ./server/config/tconfig.ts --connection -d ./server/typeorm/entity --name",
    "create": "npm run typeorm -- migration:create --config ./server/config/tconfig.ts --connection -d ./server/typeorm/migrations --name",
    "migrate": "npm run typeorm -- migration:generate --config ./server/config/tconfig.ts --connection -d ./server/typeorm/migrations --name",
    "webpack:prod": "rm -rf dist && cross-env prod=true webpack --config ./wconf/webpack.prod.js"
  },
