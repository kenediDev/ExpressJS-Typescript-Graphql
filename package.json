{
  "name": "cxfun",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm",
    "entities": "npm run typeorm -- entity:create --config ./server/utils/tconfig.ts --connection -d ./server/typeorm/entity --name",
    "create": "npm run typeorm -- migration:create --config ./server/utils/tconfig.ts --connection -d ./server/typeorm/migrations --name",
    "migrations": "npm run typeorm -- migration:generate --config ./server/utils/tconfig.ts --connection -d ./server/typeorm/migrations --name",
    "node:start": "cross-env prod=false TESTING=false ts-node server/server.ts",
    "nodemon": "cross-env prod=false TESTING=false nodemon server/server.ts",
    "node:test": "cross-env TESTING=true jest --watchAll server/**/*.test.ts",
    "kill": "pkill node",
    "soft_kill": "pkill node"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.6",
    "@angular/common": "~11.2.6",
    "@angular/compiler": "~11.2.6",
    "@angular/core": "~11.2.6",
    "@angular/forms": "~11.2.6",
    "@angular/platform-browser": "~11.2.6",
    "@angular/platform-browser-dynamic": "~11.2.6",
    "@angular/router": "~11.2.6",
    "@apollo/client": "^3.3.12",
    "apollo-angular": "^2.4.0",
    "apollo-server-express": "^2.21.2",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "class-validator": "^0.13.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "faker": "^5.4.0",
    "graphql": "^15.5.0",
    "jsonwebtoken": "^8.5.1",
    "mysql": "^2.18.1",
    "nodemailer": "^6.5.0",
    "nodemon": "^2.0.7",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "type-graphql": "^1.1.1",
    "typedi": "^0.10.0",
    "typeorm": "^0.2.31",
    "typeorm-typedi-extensions": "^0.4.1",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.5",
    "@angular/cli": "~11.2.5",
    "@angular/compiler-cli": "~11.2.6",
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.10",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/express-jwt": "^6.0.1",
    "@types/faker": "^5.1.7",
    "@types/jasmine": "~3.6.0",
    "@types/jest": "^26.0.21",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/node": "^12.20.6",
    "@types/nodemailer": "^6.4.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jest": "^26.6.3",
    "karma": "~6.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "pm2": "^4.5.5",
    "protractor": "~7.0.0",
    "ts-jest": "^26.5.4",
    "ts-node": "^8.3.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.5"
  }
}
